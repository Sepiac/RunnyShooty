} else if (keyboard_check(ord('S'))) {
    deltaY = playerSpeed;
} else if (keyboard_check(ord('D'))) {
    deltaX = playerSpeed
}

x += deltaX;
y += deltaY;

window_mouse_set(mouse_x + deltaX, mouse_y + deltaY);

if (mouse_check_button_pressed(mb_left)) {
    var bullet = instance_create(x + cos(direction), y + sin(direction), Bullet);
